class Dad :
    basketball = 1

class Son(Dad) :
    dance = 1
    def isddance(self):
        return f" Yes I dance {self.dance} no. of times"

class Grandson(Son):
    dance = 6
    def isdance(self):
        return f"Jackson yeah !!" \
               f"Yes I dasnce very well {self.dance} no. of times"
# This is called multi level inheritance where one class inherits a class which is a super class
darry = Dad()
larry = Son()
harry = Grandson()

print(harry.basketball)
# Grandson overrides the dance function is Son class

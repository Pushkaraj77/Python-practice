# Known as Lambda or Anonymous Function

minus = lambda x, y : x - y

#def minus(x, y) :
#    return x - y
print(minus(9, 4))

a = [[1, 14], [5, 6], [8, 23]]

a.sort(key = lambda x : x[1])
print(a)
# sorts in ascending or descending order

# syntax : list.sort(reverse = True\False , key = function)
